{
  "name": "MoonManager",
  "version": "1.0.0-HumanlyTested",
  "description": "Easy project management software for human workloads best fits",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Monwoo/MoonManager"
  },
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/Monwoo/MoonManager/blob/master/README.md",
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "start": "npm run env -s && ng serve --aot --proxy-config proxy.conf.js",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "postinstall": "npm run prettier -s",
    "precommit": "pretty-quick --staged",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular-mdc/web": "^0.41.3",
    "@angular/animations": "^7.1.0",
    "@angular/cdk": "^7.1.0",
    "@angular/cdk-experimental": "^7.1.0",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/flex-layout": "^7.0.0-beta.19",
    "@angular/forms": "^7.0.0",
    "@angular/http": "^7.0.0",
    "@angular/material": "^7.1.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/router": "^7.0.0",
    "@angular/service-worker": "^7.0.0",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "@ng-bootstrap/ng-bootstrap": "^3.3.0",
    "@ng-dynamic-forms/core": "^7.0.1",
    "@ng-dynamic-forms/ui-material": "^7.0.1",
    "@ngx-pwa/local-storage": "7",
    "@ngx-translate/core": "^10.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@ngx-translate/i18n-polyfill": "^1.0.0",
    "@types/detect-browser": "^3.0.0",
    "@types/lodash-es": "^4.17.1",
    "@types/yamljs": "^0.2.30",
    "angular-aframe-pipe": "^1.0.0",
    "angular-calendar": "^0.26.4",
    "angular-formio": "^3.9.0",
    "angular-material-source": "angular/material",
    "angular2-logger": "^0.7.0",
    "angular2-notifications": "^1.0.4",
    "angular2-text-mask": "^9.0.0",
    "bootstrap": "^4.1.1",
    "ccapture.js": "^1.1.0",
    "chart.js": "^2.7.3",
    "core-js": "^2.5.6",
    "date-fns": "^1.29.0",
    "detect-browser": "^3.0.1",
    "flatted": "^2.0.0",
    "fullcalendar": "^3.9.0",
    "gif.js": "^0.2.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.10",
    "lodash-es": "^4.17.11",
    "moment": "^2.22.2",
    "ngx-clipboard": "^11.1.9",
    "ngx-dropzone-wrapper": "^7.1.0",
    "ngx-papaparse": "3",
    "ngx-quill": "^4.3.0",
    "primeflex": "^1.0.0-rc.1",
    "primeicons": "^1.0.0",
    "primeng": "^6.1.7",
    "rxjs": "^6.3.1",
    "webm-writer": "^0.2.1",
    "webp-hero": "^0.0.0-dev.12",
    "yamljs": "^0.3.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.10.2",
    "@angular/cli": "~7.0.2",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/language-service": "^7.0.0",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@ngx-rocket/scripts": "^3.0.0",
    "@types/jasmine": "^2.8.7",
    "@types/jasminewd2": "^2.0.2",
    "@types/lodash": "^4.14.108",
    "@types/node": "^8.10.13",
    "codelyzer": "^4.4.4",
    "hads": "^1.7.0",
    "htmlhint": "^0.10.0",
    "https-proxy-agent": "^2.0.0",
    "husky": "^0.14.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.2",
    "karma-jasmine": "^1.1.2",
    "karma-junit-reporter": "^1.2.0",
    "prettier": "^1.14.0",
    "pretty-quick": "^1.6.0",
    "protractor": "~5.4.0",
    "puppeteer": "^1.4.0",
    "stylelint": "~9.6.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recommended-scss": "~3.2.0",
    "stylelint-config-standard": "~18.2.0",
    "stylelint-scss": "~3.3.0",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "~3.1.3"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}
